# Map static resources from the /public folder to the /assets URL path
GET           /assets/*file                controllers.Assets.versioned(path="/public", file: Asset)

GET           /cart/index                  controllers.CartController.index()
GET           /cart/create                 controllers.CartController.create()
POST          /cart/create                 controllers.CartController.createCartHandler()
GET           /cart/update/:id             controllers.CartController.update(id: Int)
POST          /cart/update                 controllers.CartController.updateCartHandler()
DELETE        /cart/delete/:id             controllers.CartController.delete(id: Int)

GET           /api/Cart                    controllers.CartController.indexJSON()
POST          /api/Cart                    controllers.CartController.createJSON()
PUT           /api/Cart                    controllers.CartController.updateJSON()
DELETE        /api/Cart/:id                controllers.CartController.deleteJSON(id: Int)

GET           /category/index              controllers.CategoryController.index()
GET           /category/create             controllers.CategoryController.create()
POST          /category/create             controllers.CategoryController.createCategoryHandler()
GET           /category/update/:id         controllers.CategoryController.update(id: Int)
POST          /category/update             controllers.CategoryController.updateCategoryHandler()
DELETE        /category/delete/:id         controllers.CategoryController.delete(id: Int)

GET           /api/Category                controllers.CategoryController.indexJSON()
POST          /api/Category                controllers.CategoryController.createJSON()
PUT           /api/Category                controllers.CategoryController.updateJSON()
DELETE        /api/Category/:id            controllers.CategoryController.deleteJSON(id: Int)

